lang='PYTHON'
print(lang[0])
print(lang[5])
print(lang[-1]) #마지막을 -1로도 설정 가능
#슬라이싱
print(lang[1:6])
print(lang[1:])
print(lang[:4]) #해당 위치의 직전까지만 출력
print(lang[:])

